<configuration>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
      <httpRuntime maxRequestLength="500240"/>
        <sessionState timeout="20" />
      <customErrors mode="On" >
        <error statusCode="404" redirect="~/404.aspx" />
      </customErrors>
    </system.web>
    
  <connectionStrings>
    <add name="veterinaria" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|BDVeterinaria.mdf;Integrated Security=True;User Instance=True"/>
  </connectionStrings>

</configuration>
